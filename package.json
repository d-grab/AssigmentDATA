{
  "name": "assigmentdata",
  "version": "1.0.0",
  "description": "Book app",
  "main": "server.js",
  "type": "module",
  "engines": { "node": "<=12.19.0" },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node backend/server",
